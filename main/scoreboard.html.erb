<div id="container">
	<table>
		<tr id="table_head">
			<th>Place</th>
			<th>Team</th>
			<th>Points</th>
			<th>School</th>
			<th>Last Scored</th>
		</tr>
		<% place = 1 %>
				<% teams = User.order(score: :desc, last_submit: :asc)%>
				<% teams.each do |team| %>
				<% if team.is_admin==0 %>
				<tr>
				  <td id="push_me"><%=place %></td>
				  <td id="push_me_please"><a href="/users/<%= team.id %>" ><%= team.username %></a></td>
				  <td id="push_me"><%= team.score %></td>
				  <td id="push_me"><%= team.school %></td>
				  <td id="push_me"><%= team.last_submit %></td>
				  <% place +=1 %>
				</tr>
              <% end %>
            <% end %>
    </table>
</div>