<% if current_user %>
<div class='container'>
    <%= form_for :event, :method => 'post' do |f| %>
      	  	<h4>Name:</h4><%= f.text_field :name, placeholder: "e.g. W.T. Woodson High School MUN", :class => "register"  %>
      	  	<h4>Date:</h4><%= f.text_field :date, placeholder: "e.g. May 17,2015", :class => "register"  %>
            <h4>About:</h4><%= f.text_area :about, size: "50x10"%>
     	    <h4></h4><%= submit_tag("Create Event!") %>
     	<% end %>
</div>
<% else %>
<div class='container'>
   <h4>Must be logged in</h4> 
</div>
<%end%>